// PV Test file

WfName="Boulder.epw"
wuDays=0
nSubSteps=1

METER Elec0

ZONE Z1
  znArea = 500.0
  znVol = 500.0 * 8
  GAIN BuildingLoad
      gnMeter = Elec0
      gnEndUse = Lit
      gnPower = 3412.142 // approximately equal to 1 kW

BATTERY "Basic"
  btMeter = Elec0
  btEndUse = BT
  btMaxCap = 10.0
  btUseUsrChg = No

PVARRAY "S_30"
  pvElecMtr = Elec0
  pvEndUse = PV
  pvDCSysSize = 5
  pvModuleType = Standard
  pvArrayType = FixedOpenRack
  pvTilt = 30
  pvAzm = 180
  pvGrndRefl = 0.2
  pvUsePVWatts = No

DELETE REPORT "EB"

EXPORTFILE "PvBtOut"
  xfFileName="PvBtOut"
  xfFileStat=OVERWRITE

EXPORT exExportFile="PvBtOut" exType=UDT exFreq=HOUR exTitle="PV+Battery Output" exDayBeg=jan 1 exDayEnd=dec 31 exHeader=ColumnsOnly
  ExportCol colhead="Day" colVal=@Top.dateStr
  ExportCol colhead="Hour" colVal=@Top.iHr
  ExportCol colhead="ST Hour" colVal=@Top.iHrST
  ExportCol colhead="Outdoor Drybulb" colVal=@Top.tDbOHrAv
  ExportCol colhead="Beam Radiation" colVal=@Top.radBeamHrAv
  ExportCol colhead="Diff. Radiation" colVal=@Top.radDiffHrAv
  ExportCol colhead="PV Array [BTU/hr]" colVal=@PVArray["S_30"].acOut
  ExportCol colhead="Battery SOE" colVal=@BATTERY["Basic"].soe
  ExportCol colhead="Battery Cycles" colVal=@BATTERY["Basic"].cycles
  ExportCol colhead="Battery Energy [kWh]" colVal=@BATTERY["Basic"].energy
  ExportCol colhead="Meter: Lighting EndUse [BTU/hr]" colVal=@METER["Elec0"].H.lit
  ExportCol colhead="Meter: Battery EndUse [BTU/hr]" colVal=@METER["Elec0"].H.bt
  ExportCol colhead="Meter: Total EndUse [BTU/hr]" colVal=@METER["Elec0"].H.tot

REPORT     rpType=MTR     rpMeter=Elec0    rpFreq=Month

RUN
